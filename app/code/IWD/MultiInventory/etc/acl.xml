<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="IWD_MultiInventory::iwdmultiinventory_warehouse" title="IWD Multi Inventory"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Catalog::catalog">
                    <resource id="Magento_Catalog::catalog_inventory">
                        <resource id="IWD_MultiInventory::iwdmultiinventory_warehouses_product" title="IWD Manage Stocks For Product"/>
                        <resource id="IWD_MultiInventory::iwdmultiinventory_warehouses" title="IWD Manage Sources"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
