<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/Magento/Mtf/Repository/etc/repository.xsd">
    <repository class="Aheadworks\SocialLogin\Test\Repository\SocialCredential">
        <dataset name="default">
            <field name="login" xsi:type="string" />
            <field name="password" xsi:type="string" />
        </dataset>
        <dataset name="twitter">
            <field name="login" xsi:type="string">%twitter_login%</field>
            <field name="password" xsi:type="string">%twitter_password%</field>
        </dataset>
        <dataset name="facebook">
            <field name="login" xsi:type="string">%facebook_login%</field>
            <field name="password" xsi:type="string">%facebook_password%</field>
        </dataset>
        <dataset name="google">
            <field name="login" xsi:type="string">%google_login%</field>
            <field name="password" xsi:type="string">%google_password%</field>
        </dataset>
        <dataset name="linkedin">
            <field name="login" xsi:type="string">%linkedin_login%</field>
            <field name="password" xsi:type="string">%linkedin_password%</field>
        </dataset>
    </repository>
</config>
